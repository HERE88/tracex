<Window x:Class="AzureRTOS.TraceManagement.Components.DeltaWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Height="185"
        Width="300"
        Title="Delta between Selected Events">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
        </Grid.RowDefinitions>

        <Canvas Grid.Row="0">
            <TextBlock Name="txtTickDiff"
                       Canvas.Top="3"
                       Canvas.Left="5"
                       Foreground="Black" />
            <TextBlock Name="txtTimeDiff"
                       Canvas.Top="33"
                       Canvas.Left="5"
                       Foreground="Black" />
        </Canvas>
    </Grid>
</Window>
